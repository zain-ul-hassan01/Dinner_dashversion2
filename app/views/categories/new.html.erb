<div class='show'>
  <h2>New Category</h2>
  <% if @category.errors.any? %>
    <%= render partial: './partial/errors', locals: {restaurant: @category} %>
  <% end %>
  <%= form_for @category, url: {action: 'create'}  do |f| %>
    <%= f.label :name%><br>
    <%= f.text_field :name%><br>
    <br>
    <div class = 'actions'>
      <%= f.submit%>
    </div>
  <%end%>
  <%= link_to 'Back', :back, class: 'link'%>
</div>